{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "inlineSources": true,
    "jsx": "react-native",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/ScreenWrapper.tsx"
  ]
}